$(document).ready(function () {
        $("#confirmBox").click(function (e) {
            // use whatever confirm box you want here
            if (!window.confirm("Are you sure to continue?"))
            {
                // preventdefault is used to stop future click operation
                e.preventDefault();
            }
        });
